{
  "private": true,
  "name": "guldgruvan",
  "author": "Marc Neander <marcneander@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcneander/guldgruvan"
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "guldgruvan",
    "react",
    "gatsby"
  ],
  "eslintConfig": {
    "extends": "@marcneander/react"
  },
  "browserslist": [
    "extends @marcneander/browserslist-config"
  ],
  "renovate": {
    "extends": [
      "config:base"
    ],
    "rebaseStalePrs": true,
    "automergeType": "branch",
    "automerge": true,
    "major": {
      "automerge": false
    }
  },
  "scripts": {
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:js": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0",
    "develop": "gatsby develop -H 0.0.0.0"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "@contentful/rich-text-types": "^13.1.0",
    "@reach/router": "^1.2.1",
    "bootstrap": "^4.3.1",
    "classnames": "2.2.6",
    "dotenv": "8.0.0",
    "gatsby": "2.7.3",
    "gatsby-image": "2.1.1",
    "gatsby-plugin-layout": "1.0.15",
    "gatsby-plugin-offline": "2.1.1",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "2.1.1",
    "gatsby-source-contentful": "2.0.63",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-sharp": "2.1.19",
    "node-sass": "^4.12.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-transition-group": "4.0.1"
  },
  "devDependencies": {
    "@marcneander/browserslist-config": "1.0.1",
    "@marcneander/eslint-config-react": "1.1.0",
    "eslint": "6.0.1",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1",
    "stylelint": "10.0.1"
  }
}
