{
  "private": true,
  "name": "guldgruvan",
  "author": "Marc Neander <marcneander@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcneander/guldgruvan"
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "guldgruvan",
    "react",
    "gatsby"
  ],
  "eslintConfig": {
    "extends": "@marcneander/react"
  },
  "stylelint": {
    "extends": "@marcneander/stylelint-config-styled-components"
  },
  "browserslist": [
    "extends @marcneander/browserslist-config"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/utils/setupTests.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".cache",
      "cypress"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/src/utils/jestPreprocess.js"
    }
  },
  "scripts": {
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:js": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0",
    "develop": "gatsby develop -H 0.0.0.0",
    "jest": "jest",
    "test": "yarn run jest && yarn run lint"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "classnames": "^2.2.6",
    "dotenv": "^6.2.0",
    "gatsby": "2.0.85",
    "gatsby-plugin-layout": "1.0.11",
    "gatsby-plugin-offline": "2.0.21",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-source-contentful": "^2.0.22",
    "gatsby-transformer-remark": "^2.1.17",
    "polished": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@marcneander/browserslist-config": "^1.0.0",
    "@marcneander/eslint-config-react": "^1.0.2",
    "@marcneander/stylelint-config-styled-components": "^1.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-gatsby": "^0.1.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "5.12.0",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "jsdom": "^13.0.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2",
    "stylelint": "^9.8.0"
  }
}
