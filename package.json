{
  "private": true,
  "name": "guldgruvan",
  "author": "Marc Neander <marcneander@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcneander/guldgruvan"
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "guldgruvan",
    "react",
    "gatsby"
  ],
  "eslintConfig": {
    "extends": "@marcneander/react"
  },
  "stylelint": {
    "extends": "@marcneander/stylelint-config-styled-components"
  },
  "browserslist": [
    "extends @marcneander/browserslist-config"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/utils/setupTests.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".cache",
      "cypress"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/src/utils/jestPreprocess.js"
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "rebaseStalePrs": true,
    "automergeType": "branch",
    "automerge": true,
    "major": {
      "automerge": false
    }
  },
  "scripts": {
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:js": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0",
    "develop": "gatsby develop -H 0.0.0.0",
    "jest": "jest",
    "test": "yarn run jest && yarn run lint",
    "gs": "gatsby info --clipboard"
  },
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "13.1.0",
    "@contentful/rich-text-types": "13.1.0",
    "babel-plugin-styled-components": "1.10.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "dotenv": "6.2.0",
    "gatsby": "2.1.31",
    "gatsby-image": "2.0.33",
    "gatsby-plugin-layout": "1.0.13",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.9",
    "gatsby-plugin-sharp": "2.0.28",
    "gatsby-plugin-styled-components": "3.0.7",
    "gatsby-source-contentful": "2.0.38",
    "gatsby-transformer-sharp": "2.1.17",
    "polished": "3.0.3",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.8.4",
    "react-helmet": "5.2.0",
    "react-transition-group": "2.6.0",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@marcneander/browserslist-config": "1.0.0",
    "@marcneander/eslint-config-react": "1.0.3",
    "@marcneander/stylelint-config-styled-components": "1.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.5.0",
    "babel-preset-gatsby": "0.1.9",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.10.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.15.1",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "jest-styled-components": "6.3.1",
    "jsdom": "14.0.0",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "stylelint": "9.10.1"
  }
}
